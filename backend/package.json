{
    "name": "data-qa-backend",
    "version": "1.0.0",
    "description": "Backend for Excel/CSV data quality analysis tool",
    "main": "server.js",
    "scripts": {
        "start": "node --max-old-space-size=8192 --expose-gc server.js",
        "dev": "nodemon --exec \"node --max-old-space-size=8192 --expose-gc server.js\"",
        "dev-debug": "nodemon --exec \"node --max-old-space-size=8192 --expose-gc --inspect server.js\""
    },
    "dependencies": {
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "cors": "^2.8.5",
        "xlsx": "^0.18.5",
        "csv-parser": "^3.0.0",
        "fs-extra": "^11.1.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.1"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}